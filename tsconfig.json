{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@modules/*": [
        "modules/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@database/*": [
        "database/modules/*"
      ],
      "@components/*": [
        "shared/components/*"
      ],
      "@assets/*": [
        "shared/assets/*"
      ],
      "@hooks/*": [
        "shared/hooks/*"
      ],
    }
  }
}